--key auth goes hard 
--to execute, use this: loadstring(game:HttpGet("https://raw.githubusercontent.com/gpuu/crackedscripts/refs/heads/main/neoxhub"))()
--only for dead rails
local oldIsFile
oldIsFile = hookfunction(isfile, function(filename)
    if filename == "newkeys.txt" then
        return true
    end
    return oldIsFile(filename)
end)

local oldReadFile
oldReadFile = hookfunction(readfile, function(filename)
    if filename == "newkeys.txt" then
        return "m4c15"
    end
    return oldReadFile(filename)
end)

game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "ATTENTION",
	Text = "LAG IS COMING IN 5 SECONDS, IT MAY CRASH!",
})
wait(5)

loadstring(game:HttpGet("https://raw.githubusercontent.com/hassanxzayn-lua/NEOXHUBMAIN/refs/heads/main/deadrails"))()
